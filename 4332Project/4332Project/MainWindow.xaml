<Window x:Class="_4332Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_4332Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="27"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="27"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button  Grid.Row="16" Width="200" x:Name="SidorovButton" Content="О авторе" Click="SidorovButton_Click"/>
        <Button Grid.Row="4" Width="200" x:Name="ZaripovButton" Content="Зарипов" Click="ZaripovButton_OnClick"/>
        <Button Grid.Row="1" Content="Новиков" Width="100" Height="30" Click="ShowAuthorInfo_Click" />
        <Button Grid.Row="5" Width="200" x:Name="SalakhovButton" Content="Салахов" Click="SalakhovButton_Click"></Button>
        <Button  Grid.Row="3" Width="100" x:Name="SulimovButton" Content="Арсений" Click="SulimovButton_Click"/>
        <Button Grid.Row="13" Width="200" x:Name="RomanovButton" Content="Романов" Click="RomanovButton_OnClick"/>
        <Button Grid.Row="14" Width="200" x:Name="SayfutdinovaButton" Content="Сайфутдинова" Click="SayfutdinovaButton_Click"/>
        <Button Grid.Row="15" Width="200" x:Name="GiniyatullinButton" Content="Гиниятуллин" Click="GiniyatullinButton_Click"></Button>
        <Button Grid.Row="10" Width="200" Background="LightPink" x:Name="MikhaylovaButton" Content="Михайлова" Click="MikhaylovaButton_Click_1"></Button>
        
        <Button Grid.Row="19"
                Width="200"
                x:Name="HananovButton"
                Content="Хананов"
                Click="HananovButton_Click"></Button>
        <Button x:Name="StepanovButton" Content="Степанов" HorizontalAlignment="Center" Margin="0,15,0,0" Grid.RowSpan="2" Grid.Row="10" VerticalAlignment="Top" Width="290" Click="StepanovButton_Click"/>
    </Grid>
</Window>
